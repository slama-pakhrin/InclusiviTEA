/* Variable Declarations */
:root {
    --stars: 5;
    --star-size: 20px;
    --star-spacing: 1.5px;
    --star-on-color: #F9DF4A;
    --star-off-color: #CCCCCC;
}

/* Rating Styles */
.rating {
    display: inline-block; /* Make the rating stars horizontal */
    padding: 10px; /* Adjust padding as needed */
}

/* Label Styles */
.rating label {
    display: inline-block; /* Make the stars display horizontally */
    width: var(--star-size);
    height: var(--star-size);
    cursor: pointer;
    transform: scaleX(-1); /* Reverse the direction of the stars */
}

.rating label:not(:first-of-type) {
    margin-left: var(--star-spacing); /* Adjust spacing between stars */
}

.rating label:before {
    content: "\2605";
    font-size: var(--star-size);
    color: var(--star-off-color);
    line-height: 1;
}

/* Input Styles */
.rating input {
    display: none;
}

/* Interaction Styles */
.rating input:checked ~ label:before,
.rating input:not(:checked) + label:hover:before,
.rating input:not(:checked) + label:hover ~ label:before {
    color: var(--star-on-color);
}
